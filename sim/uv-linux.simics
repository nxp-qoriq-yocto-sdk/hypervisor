#
# Script to run 8578/u-boot/ultravisor/linux on simulator
#

# simple guest
$guest_image  = "vmlinux.bin"
$guest_addr   = 0x20000000
$dev_tree_blob = "mpc8578sim_vmlinux.dtb"
$initrd_image  = "busybox-1.1.3"
$initrd_addr   = 0x21300000
$dev_tree_addr = 0x20f00000

run-command-file "%script%/uvsim.include"                                                                 
