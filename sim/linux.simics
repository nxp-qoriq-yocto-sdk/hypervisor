#
# Script to run 8578/u-boot/linux on bare metal
#

$kernel_image  = "uImage.8578"
$kernel_addr   = 0x1000000
$dev_tree_blob = "mpc8578sim.dtb"
$dev_tree_addr = 0x00f00000
$initrd_image  = "busybox-1.1.3.u-boot"
$initrd_addr   = 0x1300000

$boot_uimage = "true"

run-command-file "%script%/uvsim.include"
